$(function(){
    
    //contents_i
    $("#loadHeader").load("/contents_i/header.html");
    $("#loadFooter").load("/contents_i/footer.html");
    
    //pagetop
    $("a[href^=#]").click(function(){
        var Hash = $(this.hash);
        var HashOffset = $(Hash).offset().top;
        $("html,body").animate({
            scrollTop: HashOffset
        }, 500);
        return false;
    });
    
    //flickslide
   //$('#mainImages ul li').flickSlide({target:'#mainImages>ul', duration:5000});
    
    
    
    
});

